{
    "name": "pculka/aws4-auth-bundle",
    "type": "symfony-bundle",
    "description": "Allows HTTP requests to check for AWS4 signature",
    "keywords": ["bundle", "aws4", "amazon", "http"],
    "homepage": "https://github.com/pculka/aws4-auth-bundle",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/pculka/aws4-auth-bundle/issues"
    },
    "require": {
        "php": ">=5.5.0",
        "symfony/framework-bundle": "^3.0",
        "aws/aws-sdk-php": "^3.17"
    },
    "require-dev": {
        "doctrine/doctrine-bundle": "~1.0",
        "jms/serializer-bundle": "~0.11|~1.0",
        "phpunit/phpunit": "~4.0",
        "symfony/form": "~2.3|~3.0",
        "symfony/templating": "~2.1|~3.0",
        "symfony/twig-bundle": "~2.1|~3.0",
        "symfony/validator": "~2.1|~3.0"
    },
    "autoload": {
        "psr-4": {
            "PC\\Aws4AuthBundle\\": ""
        }
    }
}
